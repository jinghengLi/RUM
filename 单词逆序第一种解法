#include <string.h>

int main() {
    char str[100];
    // 使用 fgets 是安全的，但要确保指定正确的缓冲区大小
    //if (fgets(str, sizeof(str), stdin) != NULL) {
    //    // 去掉最后的换行符（如果存在）
    //    str[strcspn(str, "\n")] = '\0';
    //}
    gets(str);
    int i = 0;
    char arr[100][100]; // 用于存储分割后的字符串
    int j = 0; // 当前字符串在 arr 中的索引
    int k = 0; // 当前字符在 arr[j] 中的索引

    for (i = 0; i < strlen(str); i++) {
        if (str[i] != ' ' && str[i] != '\0') {
            arr[j][k] = str[i]; // 将字符复制到 arr[j]
            k++; // 增加 k 的值以准备存储下一个字符
        }
        else if (str[i] == ' ') {
            arr[j][k] = '\0'; // 使用空字符结束字符串
            j++; // 移动到 arr 中的下一个字符串
            k = 0; // 重置 k 以存储下一个字符串
        }
        // 不需要 else if (str[i] == '\0') 条件，因为循环会自动结束
        else if (str[i] == '\0') {
            break; // 如果遇到字符串结束符，退出循环
		}
    }
    arr[j][k] = '\0'; // 确保最后一个字符串被正确结束

//逆序输出
    for (i = j; i >= 0; i--) {
        printf("%s ", arr[i]);
}
    return 0;
}
