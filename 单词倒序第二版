#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>	
#include <string.h>

void reverse (char* left, char* right) {
    
    while (left < right) {
        char temp = *left;
        *left = *right;
        *right = temp;
        left++;
        right--;
    }
}
int main() {
    char arr[101];
    //输入
    gets(arr);
    //所有字符逆序
    reverse(arr, arr + strlen(arr) - 1);
    char* start = arr;
    while (*start) {
	    char* end = start;
        while(*end != ' ' && *end != '\0') {
            end++;
	    }
        //单词逆序
        reverse(start, end - 1);
        
        if(*end != '\0') {
            end++;
	    }
	    start = end;

    }

    printf("%s\n", arr);
    return 0;
}
