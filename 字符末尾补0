#include <stdio.h>
#include <string.h>

int main() {
    char s[101];
    scanf("%s", s);
    int len = strlen(s);
    for (int i = 0; i < len; i += 8) {
        for (int j = 0; j < 8; j++) {
            if (i + j < len) {
                putchar(s[i + j]);
            }
            else {
                putchar('0');
            }
        }
        printf("\n");
    }
    return 0;
}
