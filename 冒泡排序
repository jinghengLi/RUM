#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

void bubble_sort(int arr[], int n) {
	
	//大到小
	/*for (int i = 0; i < n; i++) {
		if (arr[i] > arr[i + 1]) {        
			int temp = arr[i];
			arr[i] = arr[i + 1];
			arr[i + 1] = temp;
		}
	}*/
	//小到大
	for (int i = 0; i < n; i++) {
		if (arr[i] < arr[i + 1]) {
			int temp = arr[i];
			arr[i] = arr[i + 1];
			arr[i + 1] = temp;
		}
	}
	printf("%d ", arr[n]);

	if (n > 0) {
		bubble_sort(arr, n - 1);
	}


	 
}
int main() {
	int arr[] = { 12,0,67,23,23,-23,78,90,-34,11 };
	int n = sizeof(arr) / sizeof(arr[0]); 
	bubble_sort(arr, n-1); 
	return 0;


}
